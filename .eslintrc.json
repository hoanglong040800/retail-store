{
  "env": {
    "browser": true,
    "es2021": true
  },
  "plugins": ["@typescript-eslint/eslint-plugin"],
  "extends": [
    "plugin:@typescript-eslint/recommended",
    "plugin:prettier/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "complexity": ["error", 3],
    "eqeqeq": "error",
    "no-unused-vars": "error",
    "no-useless-rename": "error",
    "no-var": "error"
  }
}
